{
  "name": "utilise",
  "version": "0.1.18",
  "description": "Lean JavaScript Utilities as Micro-libraries",
  "repository": {
    "type": "git",
    "url": "git://github.com/utilise/utilise.git"
  },
  "main": "index.js",
  "browser": {
    "pause": false,
    "send": false,
    "file": false,
    "via": false
  },
  "scripts": {
    "bundle": "browserify tmp.js -o utilise.js -i d3 -i colors -s utilise && rm tmp.js",
    "minify": "uglifyjs utilise.js -m -c drop_console,dead_code -d client=true -o utilise.min.js",
    "gzip": "gzip -c utilise.min.js > utilise.min.js.gz",
    "build": "node alias.js && npm run bundle && npm run minify && npm run gzip",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags"
  },
  "author": "Pedram Emrouznejad (https://github.com/pemrouz)",
  "license": "pemrouz.mit-license.org",
  "dependencies": {
    "colors": "^1.1.2",
    "through": "^2.3.8"
  },
